#pragma endregion



	int ops = 0;
	int n;

	void mat()
	{

		n = Convert::ToInt32(textBox1->Text);
		int k = Convert::ToInt32(textBox2->Text);
		int i = 0, l = 0;

		int* a = new int[n * n];

		int** b = new int* [n];

		for (int z = 0; z < n; z++)
			b[z] = new int[n];

		int m = 0;
		int j = n - 1;
		for (k,m; k < n * n + k, m < n * n; k++,m++)
			a[m] = k;

		for (k = 0; k < n * n; k++)
		{
			b[i][j] = a[k];
			if (j == n - 1 && i < n - 1 && l == 0)
			{
				i++;
				l = 1;
			}
			else if (i == 0 && j > 0 && l == 1)
			{
				j--;
				l = 0;
			}
			else if (j == 0 && l == 1)
			{
				i++;
				l = 0;
			}
			else if (i == n - 1 && l == 0)
			{
				l = 1;
				j--;
			}
			else if (l == 0)
			{
				i++;
				j++;
			}
			else
			{
				i--;
				j--;
			}
		}
		String^ s;
		for (int i = 0; i < n; i++)
		{
			for (int j = 0; j < n; j++)
			{
				s += b[i][j].ToString();
				s += "\t";
			}
			s += "\r\n";
		}
		richTextBox1->Text = s;
	}


	
	private: System::Void Exit_Click(System::Object^ sender, System::EventArgs^ e) 
	{
		this->~MyForm();
	}



	private: System::Void Settings_Click(System::Object^ sender, System::EventArgs^ e) 
	{
		this->Start->Visible = 0;
		this->Settings->Visible = 0;
		this->Exit->Visible = 0;
		this->Video->Visible = 1;
		this->Back1->Visible = 1;
		this->SSS->Visible = 1;
	}



private: System::Void Menu_KeyPress(System::Object^ sender, System::Windows::Forms::KeyPressEventArgs^ e) 
{
	if (e->KeyChar == 27 && Opacity >= 0.98)
	{
		this->Start->Visible = 1;
		this->Settings->Visible = 1;
		this->Exit->Visible = 1;
		this->SSS->Visible = 0;
		this->FullWindow->Visible = 0;
		this->FullScreen->Visible = 0;
		this->Back1->Visible = 0;
		this->Window->Visible = 0;
		this->BackSettings->Visible = 0;
		this->Video->Visible = 0;
		this->SSS->Text = L"Super Secret Setting\r\n(Danger)((Dont click on it))";
	}
	else
	{
		switch (ops)
		{
		case 1:
			Opacity += 0.25;
			this->SSS->Text = L"Горжусь тобой";
			ops -= 1;
			break;
		case 2:
			Opacity += 0.25;
			this->SSS->Text = L"Молодец";
			ops -= 1;
			break;
		}

	}
}
private: System::Void Video_Click(System::Object^ sender, System::EventArgs^ e) 
{
	this->Window->Visible = 1;
	this->FullWindow->Visible = 1;
	this->FullScreen->Visible = 1;
	this->BackSettings->Visible = 1;
	this->Back1->Visible = 0;
	this->Video->Visible = 0;
	this->SSS->Visible = 0;
}
private: System::Void BackSettings_Click(System::Object^ sender, System::EventArgs^ e) 
{
	this->Window->Visible = 0;
	this->FullWindow->Visible = 0;
	this->FullScreen->Visible = 0;
	this->BackSettings->Visible = 0;
	this->Back1->Visible = 1;
	this->Video->Visible = 1;
	this->SSS->Visible = 1;
}
	private: System::Void Back1_Click(System::Object^ sender, System::EventArgs^ e)
	{
		this->Back1->Visible = 0;
		this->Video->Visible = 0;
		this->Start->Visible = 1;
		this->Settings->Visible = 1;
		this->Exit->Visible = 1;
		this->SSS->Visible = 0;
	}



private: System::Void SSS_Click(System::Object^ sender, System::EventArgs^ e) 
{
	switch (ops)
	{
	case 0:
		Opacity -= 0.25;
		this->SSS->Text = L"Не жми, подумой !!!";
		ops += 1;
		break;
	case 1:
		Opacity -= 0.25;
		this->SSS->Text = L"Я предупреждал";
		ops += 1;
		break;
	case 2:
		this->~MyForm();
		break;
	}
}
private: System::Void Settings_KeyPress(System::Object^ sender, System::Windows::Forms::KeyPressEventArgs^ e) 
{
	this->FullWindow->Visible = 0;
	this->FullScreen->Visible = 0;
	this->Window->Visible = 0;
	this->BackSettings->Visible = 0;
	this->SSS->Visible = 1;
	this->Back1->Visible = 1;
	this->Video->Visible = 1;
}

private: System::Void Go_Click(System::Object^ sender, System::EventArgs^ e) 
{
	mat();
	this->TaDa->Visible = 1;
	this->richTextBox1->Visible = 1;
}
private: System::Void Start_Click(System::Object^ sender, System::EventArgs^ e) 
{
	this->Start->Visible = 0;
	this->Settings->Visible = 0;
	this->Exit->Visible = 0;
	this->label1->Visible = 1;
	this->textBox1->Visible = 1;
	this->Go->Visible = 1;
	this->BackMatrix->Visible = 1;
	this->textBox2->Visible = 1;
	this->label2->Visible = 1;


}

private: System::Void BackMatrix_Click(System::Object^ sender, System::EventArgs^ e) 
{
	this->Start->Visible = 1;
	this->Settings->Visible = 1;
	this->Exit->Visible = 1;
	this->label1->Visible = 0;
	this->textBox1->Visible = 0;
	this->richTextBox1->Visible = 0;
	this->BackMatrix->Visible = 0;
	this->BackMatrix->Visible = 1;
	this->TaDa->Visible = 0;
	this->Go->Visible = 0;

}
private: System::Void textBox1_KeyPress(System::Object^ sender, System::Windows::Forms::KeyPressEventArgs^ e) 
{
	if (e->KeyChar == 27)
	{
		this->Start->Visible = 1;
		this->Settings->Visible = 1;
		this->Exit->Visible = 1;
		this->label1->Visible = 0;
		this->textBox1->Visible = 0;
		this->richTextBox1->Visible = 0;
		this->BackMatrix->Visible = 0;
		this->BackMatrix->Visible = 1;
		this->TaDa->Visible = 0;
		this->Go->Visible = 0;
	}
}


private: System::Void textBox1_KeyPress_1(System::Object^ sender, System::Windows::Forms::KeyPressEventArgs^ e) 
{
	if (e->KeyChar == 27)
	{
		this->Start->Visible = 1;
		this->Settings->Visible = 1;
		this->Exit->Visible = 1;
		this->label1->Visible = 0;
		this->textBox1->Visible = 0;
		this->richTextBox1->Visible = 0;
		this->BackMatrix->Visible = 0;
		this->BackMatrix->Visible = 1;
		this->TaDa->Visible = 0;
		this->Go->Visible = 0;
	}
	if(e->KeyChar == 13)
	{
		mat();
		this->TaDa->Visible = 1;
		this->richTextBox1->Visible = 1;
	}
}
};
}
